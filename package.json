  "devDependencies": {
    "@types/config": "0.0.36",
    "@types/etag": "1.8.0",
    "@types/json-schema": "7.0.5",
    "@types/kcors": "2.2.3",
    "@types/koa": "2.11.4",
    "@types/koa-bunyan-logger": "2.1.2",
    "@types/koa-websocket": "5.0.4",
    "@types/koa__router": "8.0.2",
    "@types/mailgun-js": "0.22.10",
    "@types/node": "14.6.0",
    "@types/request-promise-native": "1.0.17",
    "@types/set-cookie-parser": "0.0.6",

    "eslint-config-prettier": "6.11.0",
    "eslint-import-resolver-babel-root-import": "0.0.2",
    "eslint-plugin-prettier": "3.1.4",
    "husky": "4.2.5",
    "lint-staged": "10.2.11",
    "nodemon": "2.0.4",
    "prettier": "2.0.5",
    "typescript": "3.9.7"
  },
